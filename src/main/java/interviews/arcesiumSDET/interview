-------------------------------------------------
Database:
Table = Students
Columns = Id, Name, Class, Subject, Marks








select * from Students Where split(" ")[0][1] == 's', 'h'

select COUNT(id) from Students GROUP BY id;
select Name, MAX(sum) from (select name, Class, SUM(Marks) as sum from Students GROUP BY id) GROUP BY Class;
