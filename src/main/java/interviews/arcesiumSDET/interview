CityWeather implements Comparable<Place>{
place,
date, weather

@Override
public int equals(Place p){
	this.place == p.place
}

@Override
public int comparator(){

}

}

public
List<Place> set1 = Xall API 1
List<Place> set2 = Xall API 2

set1.size() == set2.size()

Assert.equals(set1, set2)



set --> {1,2,3}
set2 --> {1,2,3}



public boolean areListEqual(List<CityWeather> l1, List<CityWeather> l2){

  //Have a compaaotr in city weather that will help in sorting

  //O(n2)
  //O(n) user sest for l2
  for( CityWeather c1: l1 ){
  	for( CityWeather c2: l2 ){
    	if( !(c2.place == c1.place && c2.date == c1.weather && c1.weather == c2.weather) ){
      	return false;
			}
    }
  }


}

